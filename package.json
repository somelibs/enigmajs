    "array-buffer-to-hex": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "hex-to-array-buffer": "^1.1.0",
    "lodash": "^4.17.10",
    "random-bytes": "^1.0.0"
  }
}
