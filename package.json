{
  "name": "enigmax",
  "version": "1.0.0",
  "description": "Enigma Cryptographic Library",
  "author": "The Bernstein Team",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/somelibs/enigmax.git"
  },
  "homepage": "https://github.com/somelibs/enigmax#readme",
  "bugs": {
    "url": "https://github.com/somelibs/enigmax/issues"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lib/)(?!src/)(?!config/)"
    ]
  },
  "files": [
    "dist/cjs/**",
    "dist/es/**"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir dist/cjs",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "commonjs:watch": "yarn build:commonjs -- --watch",
    "es:watch": "yarn build:es -- --watch",
    "build": "yarn build:commonjs && yarn build:es",
    "watch": "yarn commonjs:watch & yarn es:watch",
    "dev": "yarn watch & yarn lint:watch",
    "lint": "eslint src/** --fix",
    "lint:watch": "nodemon -e js -x 'yarn lint'",
    "prepare": "yarn clean && yarn build",
    "test": "BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "babel-plugin-inline-replace-variables": "^1.3.1",
    "babel-plugin-lodash": "^3.3.4",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "array-buffer-to-hex": "^1.0.0",
    "arraybuffer-to-string": "^1.0.2",
    "hex-to-array-buffer": "^2.0.0",
    "lodash": "^4.17.20",
    "random-bytes": "^1.0.0",
    "string-to-arraybuffer": "^1.0.2",
    "utf8-buffer": "^0.2.0",
    "uuid": "^9.0.0"
  }
}
