  "description": "Enigma Cryptographic Library",
  "author": "The Bernstein Team",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/bernstein-io/client.git"
  },
  "homepage": "https://github.com/bernstein-io/client#readme",
  "bugs": {
    "url": "https://github.com/bernstein-io/client/issues"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lib/)(?!src/)(?!config/)"
    ]
  },
  "main": "dist/enigma.js",
  "scripts": {
    "build": "webpack",
    "dev": "webpack --watch -d",
    "lint": "eslint src/** --fix",
    "lint:watch": "nodemon -e js -x 'npm run lint'",
    "nodemon": "nodemon --watch src -e js -x 'npm run build' --verbose",
    "test": "jest --verbose",
    "test:watch": "jest --verbose --debug --watchAll"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^23.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "nodemon": "^1.17.5",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1"
  },
