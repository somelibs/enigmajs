
{
  "plugins": [
    [ "inline-replace-variables", 
      {
        "__DEV__": {
          "type": "node",
          "replacement": "process.env.NODE_ENV !== 'production'"
        }
      }
    ],
    "@babel/plugin-proposal-export-default-from"
  ],
  "env": {
    "commonjs": {
      "plugins": [ ["@babel/plugin-transform-modules-commonjs", { "loose": true }] ]
    }
  }
}
